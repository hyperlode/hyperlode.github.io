	function openVertical(obj, upElseDown){
		var height= parseInt(obj.style.height);
		if (upElseDown){
			height -=3;
		}else{
			height +=3;
		}
		
		if ( upElseDown && height <0){
			obj.style.height = 0;
		}else if (!upElseDown && height > screen.height){
			obj.style.height = screen.height;
		}else{
			obj.style.height = height + 'px';
			animate = setTimeout( function(){openVertical(obj,upElseDown )},25);
		}
		
	}
	
	function openToSide(obj,  leftElseRight){
		
		var width = parseInt(obj.style.width);
		if (leftElseRight){
			width -= 3;
		}else{
			width +=3;
		}
		
		if ( (leftElseRight && width<0) ){
			obj.style.width = 0;
		} else if (!leftElseRight && width > screen.width ) {
			//width = screen.width;
			obj.style.width = screen.width;
		}else{
			obj.style.width = width + 'px';
			animate = setTimeout( function(){openToSide(obj,leftElseRight )},25);
		}
		
		//animate = setTimeout(openLeft,200);
		// alert(parseInt(imgObj.style.width));
	}
	
